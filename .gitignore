# -----------------------------------------------------------------------------
.idea/
.vscode/
*.swp
*.swo
*~
.project
.pydevproject
.settings/

# -----------------------------------------------------------------------------
# Jupyter Notebooks
# -----------------------------------------------------------------------------
.ipynb_checkpoints/
*.ipynb_checkpoints

# -----------------------------------------------------------------------------
